{
  "name": "prop-ops",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/mgmeyers/prop-ops",
  "author": "mgmeyers <matthew.gregory.meyers@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf lib && webpack --config webpack.config.js",
    "docs": "jsdoc2md src/*.js --plugin dmd-clear > README.md",
    "lint": "eslint src/*.js",
    "format": "prettier --config .prettierrc --write \"src/*.js\"",
    "test": "ava",
    "quality": "yarn format && yarn lint && yarn test"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@babel/register": "7.0.0",
    "ava": "1.2.0",
    "dmd-clear": "0.1.2",
    "eslint": "^5.13.0",
    "jsdoc": "3.5.5",
    "jsdoc-to-markdown": "4.0.1",
    "prettier": "1.16.3",
    "rimraf": "2.6.3"
  },
  "ava": {
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "babelrc": false,
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  }
}
